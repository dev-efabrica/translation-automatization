{
    "name": "efabrica/translation-automatization",
    "type": "library",
    "description": "Translation automatization",
    "keywords": ["PHP", "Translation", "Automatization"],
    "license": "MIT",
    "require": {
        "php": ">= 7.1",
        "guzzlehttp/guzzle": "^6.3",
        "nette/neon": "^3.3",
        "nette/utils": "^3.0",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    },
    "bin": ["bin/translation-automatization"]
}
