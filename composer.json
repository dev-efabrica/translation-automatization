{
    "name": "tonop01/translation-automatization",
    "type": "library",
    "description": "Translation automatization",
    "keywords": ["PHP", "Translation", "Automatization"],
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "php": ">=7.1",
        "nette/neon": "^2.4 | ^3.3",
        "nette/utils": "^2.5 | ^3.0",
        "symfony/console": "^4.0 | ^5.0",
        "symfony/finder": "^4.0 | ^5.0 | ^6.0",
        "nikic/php-parser": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "google/cloud-translate": "^1.12",
        "google/apiclient": "^2.12.1"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    },
    "bin": ["bin/translation-automatization"]
}
